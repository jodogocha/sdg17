<p id="notice"><%= notice %></p>

<h1>Clasifications</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clasifications.each do |clasification| %>
      <tr>
        <td><%= clasification.nombre %></td>
        <td><%= link_to 'Show', clasification %></td>
        <td><%= link_to 'Edit', edit_clasification_path(clasification) %></td>
        <td><%= link_to 'Destroy', clasification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clasification', new_clasification_path %>
